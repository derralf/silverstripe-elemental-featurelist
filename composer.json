{
    "name": "derralf/elemental-featurelist",
    "description": "A content block to display short headlines/questions and texts/answers along with icons/images for the silverstripe-elemental module",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "elemental",
        "content blocks"
    ],
    "license": "BSD-3-Clause",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "dnadesign/silverstripe-elemental": "^4",
        "silverstripe/vendor-plugin": "^1.0",
        "sheadawson/silverstripe-linkable": "^2.0@dev"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        },
        "expose": [
            "client/dist"
        ]
    },
    "autoload": {
        "psr-4": {
            "Derralf\\Elements\\Featurelist\\": "src/"
        }
    }
}
